    <!--the object property eng design incluence quality control-->
    <owl:ObjectProperty rdf:about="http://example.org/influencesQualityControl"/>

    <!-- Object Property: supports  // knowledge discovery supports the inspection process -->
    <owl:ObjectProperty rdf:about="http://example.org/supportsInspectionProcess"/>

    <!-- Object Property: utilizes  // engineering design utilizes knowledge discovery -->
    <owl:ObjectProperty rdf:about="http://example.org/utilizesKnowledgeDiscovery"/>

    <!-- Object Property: impacts  engineering design impacts thermal system engineering -->
    <owl:ObjectProperty rdf:about="http://example.org/impactsThermalSystemEngineering"/>



    <!-- Class Definition for "mechanicalEngineering" -->
    <owl:Class rdf:about="http://www.example.com/MechanicalEngineering#mechanicalEngineering">
        <rdfs:label>Mechanical Engineering Aspects</rdfs:label>
        <rdfs:comment>The class of some Mechanical Engineering Aspects</rdfs:comment>
    </owl:Class>

    <!-- Subclass Definition for "Engineering Design" -->
    <owl:Class rdf:about="http://www.example.com/MechanicalEngineering#EngineeringDesign">
        <rdfs:subClassOf rdf:resource="http://www.example.com/MechanicalEngineering#EngineeringDesign"/>
        <rdfs:label>Engineering design aspect</rdfs:label>
        <rdfs:comment>Aspect related to designing, simulating, and optimizing mechanical systems.</rdfs:comment>
    </owl:Class>

    <!-- Subclass Definition for "Knowledge Discovery" -->
    <owl:Class rdf:about="http://www.example.com/MechanicalEngineering#KnowledgeDiscovery">
        <rdfs:subClassOf rdf:resource="http://www.example.com/MechanicalEngineering#KnowledgeDiscovery"/>
        <rdfs:label> Aspect Knowledge Discovery</rdfs:label>
        <rdfs:comment>facilitate data integration, semantic search, and knowledge discovery in mechanical engineering</rdfs:comment>
    </owl:Class>

    <!-- Subclass Definition for "Quality control" -->
    <owl:Class rdf:about="http://www.example.com/MechanicalEngineering#Qualitycontrol">
        <rdfs:subClassOf rdf:resource="http://www.example.com/MechanicalEngineering#Qualitycontrol"/>
        <rdfs:label> Aspect Quality control</rdfs:label>
        <rdfs:comment>track quality parameters, inspection processes, and compliance with industry regulations</rdfs:comment>
    </owl:Class>

    <!-- Subclass Definition for "Education and Training" -->
    <owl:Class rdf:about="http://www.example.com/MechanicalEngineering#EducationandTraining">
        <rdfs:subClassOf rdf:resource="http://www.example.com/MechanicalEngineering#EducationandTraining"/>
        <rdfs:label> Aspect Education and Training</rdfs:label>
        <rdfs:comment> understanding of mechanical engineering concepts and best practices.</rdfs:comment>
    </owl:Class>

    <!-- Subclass Definition for "Thermal Systems Engineering" -->
    <owl:Class rdf:about="http://www.example.com/MechanicalEngineering#ThermalSystemsEngineering">
        <rdfs:subClassOf rdf:resource="http://www.example.com/MechanicalEngineering#ThermalSystemsEngineering"/>
        <rdfs:label> Aspect Thermal Systems Engineering</rdfs:label>
        <rdfs:comment> designing efficient heating, ventilation, and air conditioning systems, simulating their performance.</rdfs:comment>
    </owl:Class>

    <!-- sub classes of eng design-->

    <!-- Subclass Definitions for "Simulation," "Optimization," and "Designing Mechanical Systems" -->
    <owl:Class rdf:about="http://www.example.com/MechanicalEngineering#Simulation">
        <rdfs:subClassOf rdf:resource="http://www.example.com/MechanicalEngineering#EngineeringDesign"/>
        <rdfs:label>Simulation aspect</rdfs:label>
        <rdfs:comment>Aspect related to simulating mechanical systems.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://www.example.com/MechanicalEngineering#Optimization">
        <rdfs:subClassOf rdf:resource="http://www.example.com/MechanicalEngineering#EngineeringDesign"/>
