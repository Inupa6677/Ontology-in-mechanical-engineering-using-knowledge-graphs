        <rdfs:label>Designing Mechanical Systems aspect</rdfs:label>
        <rdfs:comment>Aspect related to designing mechanical systems.</rdfs:comment>
    </owl:Class>

    <!-- Object Property Definition for "IsLinkedTo" -->
    <owl:ObjectProperty rdf:about="http://www.example.com/MechanicalEngineering#IsLinkedTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label>Is Linked To</rdfs:label>
        <rdfs:comment>Property indicating a link between different aspects of mechanical engineering.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.example.com/MechanicalEngineering#Simulation"/>
        <rdfs:range rdf:resource="http://www.example.com/MechanicalEngineering#EngineeringDesign"/>
    </owl:ObjectProperty>

    <!-- Object Property Definition for "Involves" -->
    <owl:ObjectProperty rdf:about="http://example.com/MechanicalEngineering#Involves">
    <rdfs:label>Involves</rdfs:label>
    <rdfs:comment>Indicates the involvement of one instance in another.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Object Property Definition for "considers" -->
    <owl:ObjectProperty rdf:about="http://example.com/MechanicalEngineering#Considers">
    <rdfs:label>Considers</rdfs:label>
    <rdfs:comment>Indicates that one concept considers another in the context of designing mechanical systems.</rdfs:comment>
    </owl:ObjectProperty>



        <!-- Subclass Definitions for data integration, sementic search and kniwledge discovery tecnique-->
