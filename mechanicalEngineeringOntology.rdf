        <dc:description>An example ontology for mechanical engineering</dc:description>
    </owl:Ontology>

    <!-- Object Definitions-->
    <owl:ObjectProperty rdf:about="http://example.com/MechanicalEngineering#IsLinkedTo"/>
    <owl:ObjectProperty rdf:about="http://example.com/MechanicalEngineering#involves"/>
    <owl:ObjectProperty rdf:about="http://example.com/MechanicalEngineering#Considersin"/>
    <owl:ObjectProperty rdf:about="http://example.com/MechanicalEngineering#isusedin"/>
    <owl:ObjectProperty rdf:about="http://example.com/MechanicalEngineering#isusedin"/>
    <owl:ObjectProperty rdf:about="http://example.com/MechanicalEngineering#isappliedin"/>
    <owl:ObjectProperty rdf:about="http://example.com/MechanicalEngineering#isfollowedby"/>
